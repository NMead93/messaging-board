{{#each model as |question|}}
    <div class="panel panel-default panel-horizontal">
        <div class="panel-heading text-center" style="width:10em;">
            <span>{{question.date}}</span>
            <hr />
            <div class="email" style="padding-top: 10px;">{{question.author}}</div>
        </div>

        <div class="panel-body">

    <!--div class="col-xs-2 col-md-3 text-center age">
        <img src="https://placeholdit.imgix.net/~text?txtsize=40&txt=John%20Doe&w=200&h=200" class="img-circle img-responsive" alt="" />
    </div-->
    <div class="col-xs-12 col-md-12 section-box">
        <h2>
            {{question.title}} <a href="http://bootsnipp.com/" target="_blank"><span class="glyphicon glyphicon-new-window">
            </span></a>
        </h2>
        <p>
            {{question.body}}
        </p>
        <hr />
        <div class="row rating-desc">
            <div class="col-md-12">
                <span class="glyphicon glyphicon-comment"></span>(100 Comments)<span class="separator">|</span>
            </div>
        </div>
    </div>

        </div>
        <div class="panel-footer text-center" style="width:4em;">
            <button class="btn btn-warning">Edit</button>
        </div>
    </div>
{{/each}}
